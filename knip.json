{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "project": ["src/**/*.{js,ts,tsx}", "scripts/**/*.{js,ts}"],
    "vite": {
        "config": ["vite.config.ts"]
    },
    "rules": {
        "files": "off",
        "dependencies": "off",
        "unlisted": "off",
        "binaries": "off",
        "unresolved": "off",
        "exports": "error",
        "types": "off",
        "nsExports": "off",
        "nsTypes": "off",
        "enumMembers": "off",
        "classMembers": "off",
        "duplicates": "off"
    }
}
